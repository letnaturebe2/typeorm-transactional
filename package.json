{
  "name": "typeorm-transactional-service",
  "version": "1.0.1",
  "description": "TypeORM @Transactional decorator implementation",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:all": "jest",
    "build": "tsc",
    "lint": "biome check .",
    "lint:fix": "biome check . --write",
    "format": "biome format . --write",
    "format:check": "biome format . --check"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.14"
  },
  "peerDependencies": {
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@nestjs/common": "^11.1.6",
    "@nestjs/core": "^11.1.6",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/testing": "^11.1.6",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.5.0",
    "@types/supertest": "^6.0.3",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "express": "^5.1.0",
    "jest": "^29.6.2",
    "rxjs": "^7.8.2",
    "sqlite3": "^5.1.6",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typeorm": "^0.3.17",
    "typeorm-naming-strategies": "^4.1.0",
    "typescript": "^5.1.6"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/examples/tests"
    ],
    "testMatch": [
      "**/__tests__/**/*.ts",
      "**/*.test.ts"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  },
  "files": [
    "dist/src/index.js",
    "dist/src/index.d.ts",
    "dist/src/decorators/",
    "dist/src/types/"
  ],
  "keywords": [
    "typeorm",
    "transaction",
    "decorator",
    "database",
    "typescript"
  ],
  "author": "letnaturebe2@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/letnaturebe2/typeorm-transactional"
  }
}
